<body>

    <!-- Wrapper -->
    <div class="hk-wrapper" data-layout="vertical" data-layout-style="default" data-menu="dark" data-footer="simple">
        <!-- Top Navbar -->
        <nav class="hk-navbar navbar navbar-expand-xl navbar-light fixed-top">
            <div class="container-fluid">
                <!-- End Nav -->
                <div class="nav-end-wrap">
                    <ul class="navbar-nav flex-row">
                        <li class="nav-item">

                            <div id="google_translate_element"></div>
                            <script src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
                            <script>
                                function googleTranslateElementInit() {
                                    new google.translate.TranslateElement({
                                        pageLanguage: 'vi',
                                        // includedLanguages: 'cs,de,en,es,fr,fy,hi,hmn,hu,id,ja,km,kn,ko,la,lo,mg,ml,mn,ms,ne,pt,ro,ru,th,tr,uk,vi,zh-CN,zh-TW',
                                        layout: google.translate.TranslateElement.InlineLayout.HORIZONTAL,
                                        // multilanguagePage: true
                                    }, 'google_translate_element');
                                }
                            </script>
                        </li>
                    </ul>
                </div>
                <!-- /End Nav -->
            </div>
        </nav>
        <!-- /Top Navbar -->
    </div>
    <script src="/jampack/dist/js/script.js?=1"></script>

    <script src="/jampack/dist/js/user.js?=3"></script>
    <script>
        $(document).ready(function () {
            // if (!getCookie('modalSystem')) {
            //     $('#modalSystem').modal('show');
            // }
            // $('#modalSystem').modal('show');
            $('#modalSystem').modal('show');

            $.ajax({
                type: "POST",
                url: "https://subgiare.vn/statistical-orders",
                data: {},
                dataType: "json",
                success: function (response) {
                    if (response.status) {
                        let data = response.data;
                        $('#totalOrder').html(formatNumber(data.total));
                        $('#totalActive').html(formatNumber(data.active));
                        $('#totalSuccess').html(formatNumber(data.success));
                        $('#totalPause').html(formatNumber(data.pause));
                        $('#totalReport').html(formatNumber(data.report));
                        $('#totalRefund').html(formatNumber(data.refund));
                        let showProcess = $('#showProcess');
                        showProcess.html(`${data.process}%`);
                        showProcess.attr('style', `width: ${data.process}%`);
                        showProcess.attr('aria-valuenow', data.process);
                    }
                }
            });

        });

        function closeModalSystem() {
            setCookie('modalSystem', true, 1);
            $('#modalSystem').modal('hide');
        }
    </script>
</body>

</html>
